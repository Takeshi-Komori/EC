<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>kagoyume.Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResistrationConfirm</servlet-name>
        <servlet-class>kagoyume.ResistrationConfirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginJudge</servlet-name>
        <servlet-class>kagoyume.LoginJudge</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Resistration</servlet-name>
        <servlet-class>kagoyume.Resistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>kagoyume.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ItemAdd</servlet-name>
        <servlet-class>kagoyume.ItemAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResistrationConfirm</servlet-name>
        <url-pattern>/ResistrationConfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginJudge</servlet-name>
        <url-pattern>/LoginJudge</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Resistration</servlet-name>
        <url-pattern>/Resistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ItemAdd</servlet-name>
        <url-pattern>/ItemAdd</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
